{
  "addr": ":8001",

  "centrifugo": {
    "url": "centrifugo:10000",
    "tryOnChannel": "try-on:user#",
    "processingChannel": "processing:user#"
  },

  "static": {
    "type": "http",
    "dir": "static",
    "clothes": "clothes",
    "cut": "clothes/cut",
    "fullBody": "photos",
    "tryOn": "try-on",
    "s3": {
      "endpoint": "https://hb.ru-msk.vkcs.cloud",
      "bucket": "try_on_wardrobe_images"
    },
    "httpApi": {
      "endpoint": "http://model-api:8000/aws",
      "uploadUrl": "/upload",
      "deleteUrl": "/delete",
      "tokenHeader": "X-ACCESS-TOKEN"
    }
  },

  "rabbit": {
    "host": "wardrobe-rabbitmq",
    "port": 5672,
    "user": "usr",
    "try_on": {
      "request": "model",
      "response": "model_response"
    },
    "process": {
      "request": "model",
      "response": "model_response"
    }
  },

  "sql": {
    "dir": "scripts/sql/migrations"
  },

  "session": {
    "tokenName": "X-Session-ID",
    "maxAge": 86400,
    "secret": "secret"
  },

  "postgres": {
    "user": "try-on-admin",
    "db": "try-on",
    "host": "postgres",
    "port": 5432,
    "maxConn": 100,
    "initTimeout": 10
  },

  "cors": {
    "domain": "http://127.0.0.1:5500",
    "allowCredentials": true,
    "maxAge": 0
  }
}
