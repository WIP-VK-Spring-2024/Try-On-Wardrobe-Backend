{
  "addr": ":8001",

  "static": {
    "type": "fs",
    "dir": "static",
    "clothes": "clothes",
    "cut": "clothes/cut",
    "fullBody": "people",
    "tryOn": "try-on"
  },

  "rabbit": {
    "host": "wardrobe-rabbitmq",
    "port": 5672,
    "user": "usr",
    "requestQueue": "model",
    "responseQueue": "model_response"
  },

  "sql": {
    "dir": "scripts/sql/migrations"
  },

  "s3": {
    "endpoint": "https://hb.ru-msk.vkcs.cloud",
    "bucket": "try_on_wardrobe_images"
  },

  "session": {
    "tokenName": "X-Session-ID",
    "maxAge": 86400,
    "secret": "secret"
  },

  "postgres": {
    "user": "try-on-admin",
    "db": "try-on",
    "host": "postgres",
    "port": 5432,
    "maxConn": 100,
    "initTimeout": 10
  },

  "cors": {
    "domain": "http://127.0.0.1:5500",
    "allowCredentials": true,
    "maxAge": 0
  }
}
